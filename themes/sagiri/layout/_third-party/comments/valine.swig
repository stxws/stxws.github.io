	<script>
		// 根据表情的emoji_size参数设置表情的大小
		function change_emoji_szie()
		{
			var emoji_list = document.getElementsByClassName("vemoji");
			for (var i = 0; i < emoji_list.length; i++)
			{
				var emoji_elem = emoji_list[i];
				var emoji_size = /emoji_size=[0-9]+/.exec(emoji_elem.src);
				if (emoji_size != null)
				{
					emoji_size = emoji_size[0].substring(11);
					emoji_elem.style.width = emoji_size + "px";
					emoji_elem.style.height = emoji_size + "px";
				}
			}
		}
		setInterval(change_emoji_szie, 1000);
	</script>
